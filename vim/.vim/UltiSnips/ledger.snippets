snippet fold "Foldin" b
;; ${1:marking} {{{
${0:${VISUAL:# something}}
;; }}} $1
endsnippet

snippet w "Week" b
;; Week ${1:xx} {{{
${0:${VISUAL:# something}}
;; }}} Week $1
endsnippet

snippet h "Month"
========== ${1:Mmm} '${2:YY} ==========$0
endsnippet

snippet m "Month" b
